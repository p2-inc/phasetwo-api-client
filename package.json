{
  "name": "phasetwo-api-client",
  "version": "0.1.0",
  "description": "Isomorphic JavaScript client for interacting with the Phase Two API",
  "repository": {
    "type": "git",
    "url": "https://github.com/p2-inc/phasetwo-api-client.git"
  },
  "private": true,
  "source": "src/index.js",
  "main": "dist/phasetwo-api-client.js",
  "module": "dist/phasetwo-api-client.es.js",
  "umd:main": "dist/phasetwo-api-client.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle --name PhasetwoApiClient --no-sourcemap",
    "build:debug": "npm run build -- --output debug --no-compress",
    "dev": "microbundle watch",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "release": "np",
    "test": "jest",
    "test-watch": "npm test -- --watchAll"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "microbundle": "^0.12.0",
    "np": "^6.2.3"
  }
}
